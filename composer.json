{
    "name": "Ruchikumari94/iitbacademic",
    "description": "IITB Academic Profile",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
       "composer/installers": "^1.0.24",
       "nitesh624/academix": "dev-master",
       "drupal/admin_toolbar": "~1.24",
       "drupal/name": "~1.0",
       "drupal/token": "~1.3",
       "drupal/smtp": "~1.0",
       "drupal/computed_field": "~2.0",
       "drupal/search_api_solr": "~2.0",
       "drupal/search_api_autocomplete": "~1.0",
       "drupal/facets": "~1.0",
       "drupal/coffee": "~1.0",
       "drupal/devel": "~1.0",
       "drupal/migrate_tools": "~2.0",
       "drupal/migrate_plus": "~2.0",
       "drupal/migrate_source_csv": "~2.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/nitesh624/academix"
        }
    ],
    "extra": {
    	"installer-paths": {
        "profiles/contrib/iitbacademic/modules/contrib/{$name}": ["type:drupal-module"],
        "profiles/contrib/iitbacademic/themes/{$name}": ["type:drupal-theme"]   
    	}
    }
}
